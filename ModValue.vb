Public Class ModValue

    Public Property Name As String

    Public Sub getname()
        Name.ToString()
    End Sub

    Public Sub setname(ByVal value As String)
        Name = value
    End Sub

    Public Property Value As String

    Public Sub getvalue()
        Value.ToString()
    End Sub

    Public Sub setvalue(ByVal value As String)
        value = value
    End Sub
End Class